{
    "name": "Lidarr",
    "version": "arm64v8-latest",
    "slug": "lidarr",
    "description": "Lidarr is a music collection manager for Usenet and BitTorrent users",
    "url": "https://hub.docker.com/r/linuxserver/lidarr",
    "image": "linuxserver/lidarr",
    "webui": "https://[HOST]:[PORT:8686]",
    "startup": "application",
    "arch": ["aarch64"],
    "ports": {
        "8686/tcp": 8686
    },
    "environment": {
        "PUID": "1000",
        "PGID": "1000",
        "TZ": "Etc/UTC"
    },
    "map": [ "share:rw", "ssl:ro", "media:rw" ],
    "services": [],
    "options": {
    },
    "schema": {
    }
}
